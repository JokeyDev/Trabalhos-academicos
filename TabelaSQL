SELECT * FROM ALUNOS;
SELECT NOME, TELEFONE FROM ALUNOS;
SELECT * FROM ALUNOS WHERE IDADE > 25;
SELECT * FROM ALUNOS WHERE CIDADE = 'São Paulo' AND IDADE <= 30;
SELECT * FROM ALUNOS ORDER BY NOME ASC;
SELECT * FROM ALUNOS ORDER BY IDADE DESC LIMIT 5;
UPDATE ALUNOS SET TELEFONE = '11999999999' WHERE ID_ALUNO = 3;
DELETE FROM ALUNOS WHERE ID_ALUNO = (SELECT MIN(ID_ALUNO) FROM ALUNOS);
SELECT COUNT(*) AS TOTAL_ALUNOS FROM ALUNOS;
SELECT AVG(IDADE) AS IDADE_MEDIA, MAX(IDADE) AS IDADE_MAXIMA, MIN(IDADE) AS
IDADE_MINIMA FROM ALUNOS;
CREATE TABLE IF NOT EXISTS ALUNOS_NOVOS AS
SELECT * FROM ALUNOS WHERE IDADE > 25;
ALTER TABLE ALUNOS ADD COLUMN STATUS TEXT DEFAULT 'Ativo';
UPDATE ALUNOS SET STATUS = 'Ativo' WHERE STATUS IS NULL;
