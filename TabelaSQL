CREATE TABLE IF NOT EXISTS ALUNOS (
 ID_ALUNO INTEGER PRIMARY KEY AUTOINCREMENT,
 NOME TEXT,
 IDADE INTEGER,
 CIDADE TEXT,
 TELEFONE TEXT,
 DATA_CADASTRO DATE
);
